{
  "name": "correlaid-org",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "^16.14 || >=18"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && node replace_urls.js",
    "preview": "vite preview",
    "format": "npx prettier -w .",
    "format_check": "npx prettier --check .",
    "quick_format": "npx pretty-quick --staged",
    "lint": "npx eslint src/",
    "precommit": "run-s quick_format lint",
    "init_pre_commit": "husky install",
    "test": "npx vitest run",
    "e2e": "npx playwright test",
    "serve-static": "npx serve ${BUILD_DIR:-.svelte-kit/cloudflare}",
    "build-and-test": "run-s build e2e",
    "remove-console-log": "npx jscodeshift ./ --extensions=svelte,js -t codeshift/remove-console.mjs -p --ignore-config=codeshift/.shiftignore"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.4",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/svelte": "^5.2.8",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.27.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^16.2.0",
    "husky": "^8.0.3",
    "jscodeshift": "^0.15.2",
    "jsdom": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "pretty-quick": "^4.1.1",
    "vitest": "^1.6.1"
  },
  "type": "module",
  "dependencies": {
    "@event-calendar/core": "^4.3.0",
    "@event-calendar/day-grid": "^3.12.0",
    "@humanspeak/svelte-markdown": "^0.8.6",
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/adapter-cloudflare": "^7.0.3",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/adapter-vercel": "^5.7.2",
    "@sveltejs/kit": "^2.21.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.16",
    "cheerio": "^1.0.0",
    "detect-element-overflow": "^1.4.2",
    "dotenv": "^16.5.0",
    "he": "^1.2.0",
    "highlight.js": "^11.11.1",
    "i18n": "^0.15.1",
    "ical-generator": "^6.0.1",
    "lodash": "^4.17.21",
    "maplibre-gl": "^2.4.0",
    "moment-timezone": "^0.5.48",
    "postcss": "^8.5.3",
    "proxyquire": "^2.1.3",
    "serve": "^14.2.4",
    "simply-reactive": "^4.1.0",
    "svelte": "^5.33.1",
    "svelte-carousel": "github:pratham2609/svelte-carousel",
    "svelte-highlight": "^7.8.3",
    "svelte-preprocess": "^6.0.3",
    "svelte-select": "^5.8.3",
    "tailwindcss": "^3.4.17",
    "tiny-glob": "^0.2.9",
    "undici": "^5.29.0",
    "uuid": "^9.0.1",
    "vite": "^5.4.19",
    "yup": "^0.32.11"
  }
}
