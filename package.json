{
  "name": "correlaid-org",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "^16.14 || >=18"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && node dl_external_imgs.js",
    "preview": "vite preview",
    "format": "npx prettier -w .",
    "format_check": "npx prettier --check .",
    "quick_format": "npx pretty-quick --staged",
    "lint": "npx eslint src/",
    "precommit": "run-s quick_format lint",
    "init_pre_commit": "husky install",
    "test": "npx vitest run",
    "e2e": "npx playwright test",
    "serve-static": "npx serve .svelte-kit/cloudflare",
    "build-and-test": "run-s build e2e",
    "remove-console-log": "npx jscodeshift ./ --extensions=svelte,js -t codeshift/remove-console.mjs -p --ignore-config=codeshift/.shiftignore"
  },
  "devDependencies": {
    "@playwright/test": "^1.33.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/adapter-vercel": "^2.3.0",
    "@sveltejs/kit": "^1.10.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.39.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.3",
    "jscodeshift": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^2.8.7",
    "prettier-plugin-svelte": "^2.10.0",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "pretty-quick": "^3.1.3",
    "serve": "^14.2.0",
    "svelte": "^3.55.1",
    "svelte-carousel": "^1.0.25",
    "svelte-turnstile": "^0.3.1",
    "sveltemod": "^0.0.2",
    "tailwindcss": "^3.2.7",
    "vite": "^4.5.3",
    "vitest": "^0.30.1"
  },
  "type": "module",
  "dependencies": {
    "@sveltejs/adapter-cloudflare": "^2.2.2",
    "@tailwindcss/forms": "^0.5.3",
    "cheerio": "^1.0.0-rc.12",
    "detect-element-overflow": "^1.4.2",
    "dotenv": "^16.0.3",
    "he": "^1.2.0",
    "highlight.js": "^11.8.0",
    "i18n": "^0.15.1",
    "ical-generator": "^6.0.1",
    "lodash": "^4.17.21",
    "maplibre-gl": "^2.4.0",
    "nodemailer": "^6.9.1",
    "proxyquire": "^2.1.3",
    "simply-reactive": "^4.1.0",
    "svelte-email": "^0.0.4",
    "svelte-highlight": "^7.2.1",
    "svelte-markdown": "^0.2.3",
    "svelte-media-queries": "^1.5.4",
    "svelte-prism": "^1.1.6",
    "svelte-select": "^5.6.1",
    "undici": "^5.28.3",
    "uuid": "^9.0.1",
    "yup": "^0.32.11"
  }
}
