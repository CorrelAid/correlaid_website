{
  "name": "correlaid-org",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "^16.14 || >=18"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && node replace_urls.js",
    "preview": "vite preview",
    "format": "npx prettier -w .",
    "format_check": "npx prettier --check .",
    "quick_format": "npx pretty-quick --staged",
    "lint": "npx eslint src/",
    "precommit": "run-s quick_format lint",
    "init_pre_commit": "husky install",
    "test": "npx vitest run",
    "e2e": "npx playwright test",
    "serve-static": "npx serve ${BUILD_DIR:-.svelte-kit/cloudflare}",
    "build-and-test": "run-s build e2e",
    "remove-console-log": "npx jscodeshift ./ --extensions=svelte,js -t codeshift/remove-console.mjs -p --ignore-config=codeshift/.shiftignore"
  },
  "devDependencies": {
    "@humanwhocodes/config-array": "^0.13.0",
    "@playwright/test": "^1.33.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^5.2.1",
    "autoprefixer": "^10.4.13",
    "eslint": "^9.27.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte": "^2.45.1",
    "husky": "^8.0.3",
    "jscodeshift": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "pretty-quick": "^4.0.0",
    "vitest": "^1.0.0"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.726.0",
    "@event-calendar/core": "^3.4.0",
    "@event-calendar/day-grid": "^3.4.0",
    "@humanspeak/svelte-markdown": "^0.8.6",
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/adapter-cloudflare": "^3.0.0",
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/adapter-vercel": "^4.0.0",
    "@sveltejs/kit": "^2.5.27",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "cheerio": "^1.0.0-rc.12",
    "detect-element-overflow": "^1.4.2",
    "dotenv": "^16.0.3",
    "he": "^1.2.0",
    "highlight.js": "^11.8.0",
    "i18n": "^0.15.1",
    "ical-generator": "^6.0.1",
    "lodash": "^4.17.21",
    "maplibre-gl": "^2.4.0",
    "moment-timezone": "^0.5.45",
    "postcss": "^8.4.31",
    "proxyquire": "^2.1.3",
    "serve": "^14.2.0",
    "simply-reactive": "^4.1.0",
    "svelte": "^5.0.0",
    "svelte-carousel": "^1.0.25",
    "svelte-highlight": "^7.2.1",
    "svelte-preprocess": "^6.0.3",
    "svelte-select": "^5.6.1",
    "tailwindcss": "^3.2.7",
    "undici": "^5.28.4",
    "uuid": "^9.0.1",
    "vite": "^5.4.4",
    "yup": "^0.32.11"
  }
}
